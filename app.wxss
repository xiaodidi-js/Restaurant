/* 字体大小 */

.domStyle {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 11;
}

/* confirm-popo start */

.confirm-popo {
  position: absolute;
  top: 0px;
  left: 0px;
  right: 0px;
  width: 70%;
  margin: 0px auto;
  background: #fff;
  text-align: center;
  border-radius: 5px;
  z-index: 13;
}

.confirm-popo .popo-h1 {
  margin-top: 10px;
  line-height: 25px;
  padding: 15px 50px;
}

.confirm-popo .popo-h2 {
  line-height: 30px;
  text-align: center;
  margin: 0px 0px 5px;
}

.confirm-popo .popo-h3 .confirm-btn, .cancel-btn {
  width: 50%;
  height: 40px;
  line-height: 40px;
  border-radius: 0px;
  background: #fff;
  position: unset;
  border-top: 1px solid #eee;
}

.confirm-popo .popo-h3 .confirm-btn {
  border-radius: 0px 0px 5px 0px;
  color: #ff8023;
}

.confirm-popo .popo-h3 .cancel-btn {
  border-radius: 0px 0px 0px 5px;
  border-right: 1px solid #eee;
}

/* confirm-popo end */
