.nav_main .shotcut {
  width: 33%;
  height: 100%;
  float: left;
}

.nav_main .shotcut-body {
  float: left;
  height: 100%;
  position: relative;
  margin-left: 10px;
  width: 62%;
}

.nav_main .shotcut-body .shotcut-title {
  overflow: hidden;
  text-overflow: ellipsis;
  height: 28px;
  line-height: 20px;
}

.nav_main .shotcut-body .shotcut-key {
  font-size: 24rpx;
  margin-top: -6px;
}

.nav_main .shotcut-body .shotcut-money {
  position: absolute;
  top: 55px;
  height: auto;
  color: #ff8023;
  font-size: 28rpx;
}

.nav_main .shotcut-body .num-counter {
  border: 1px solid #dedede;
  border-radius: 100px;
  float: right;
  /* width: 180rpx; */
  height: 55rpx;
  margin: 8px 5px 0px 0px;
}

.nav_main .shotcut-body .num-counter .btns {
  color: #ff8023;
  width: 20px;
  height: 55rpx;
  font-weight: bold;
  line-height: 55rpx;
  text-align: center;
  font-size: 25rpx;
}

.nav_main .shotcut-body .num-counter .addnumber {
  text-align: center;
  color: #727171;
  font-weight: bold;
  font-size: 25rpx;
  width: 23px;
  height: 100%;
  line-height: 100%;
  padding: 0px 8px;
}

/* confirm start */

.confirm {
  position: fixed;
  bottom: 0px;
  left: 0px;
  height: 45px;
  width: 100%;
  background: #fff;
}

.confirm .chonseok {
  width: 45%;
  height: 100%;
  float: right;
  background: #ff8023;
  border-radius: 0px;
  color: #fff;
  font-size: 16px;
  line-height: 45px;
}

.confirm .allmoney {
  width: 55%;
  float: left;
  height: 100%;
  line-height: 40px;
  border-top: 1px solid #eee;
}

.confirm .allmoney .group-cart {
  width: 39px;
  height: 39px;
  background: #ff8023;
  border-radius: 100px;
  text-align: center;
  line-height: 39px;
  color: #fff;
  float: left;
  margin: 3px 0px 0px 30px;
  position: relative;
}

.confirm .allmoney .group-cart .icon-icon {
  font-size: 22px;
}

.confirm .allmoney .group-cart .add-number {
  position: absolute;
  right: -14px;
  top: 0px;
  width: 22px;
  height: 22px;
  line-height: 22px;
  border-radius: 100px;
  background: #f25647;
  font-size: 12px;
}

.confirm .allmoney .money {
  color: #ff8023;
  font-size: 40rpx;
  line-height: 45px;
  margin-left: 20px;
}

/* confirm end */

/* popup start */

.popup {
  position: fixed;
  left: 0px;
  right: 0px;
  top: 0px;
  width: 80%;
  height: auto;
  background: #fff;
  border-radius: 10px;
  margin: 0px auto;
  padding: 0px 0px 10px;
  z-index: 12;
}

.popup .popup-image {
  width: 100%;
  height: 210px;
}

.popup .popup-image image {
  border-radius: 10px 10px 0px 0px;
  position: relative;
}

.popup .popup-image .icon-cha {
  position: absolute;
  top: 0px;
  right: 0px;
  padding: 5px 5px 0px 0px;
  font-size: 30px;
  color: #999;
  border-radius: 0px 5px 0px 0px;
}

.popup .popup-title {
  text-align: center;
  font-size: 40rpx;
  margin-top: 20px;
  font-family: 'microsoft yahei';
}

.popup .popupo-guige {
  /* margin: 10px 10px 0px; */
  height: inherit;
  width: 90%;
  margin: 10px auto 0px;
}

.popup .popupo-guige .popup-head {
  display: block;
  width: 100%;
  font-size: 24rpx;
}

.popup .popupo-guige .guige-list {
  float: left;
  padding: 8px 9px;
  border: 1px solid #9fa0a0;
  color: #9fa0a0;
  border-radius: 50px;
  margin: 10px 5px 0px;
  font-size: 24rpx;
}

.popup .popup-money {
  border-top: 1px solid #dedede;
  height: 50px;
  position: relative;
  top: 10px;
  clear: both;
}

.popup .popup-money .storageMoney {
  font-weight: bold;
  color: #ff8023;
  line-height: 40px;
  font-size: 26px;
  font-family: 'Microsoft yahei';
  margin: 10px 35px;
}

.popup .popup-money .chonsesOK {
  background: #ff8023;
  border-radius: 5px;
  color: #fff;
  width: 107px;
  height: 37px;
  line-height: 37px;
  margin: 11px 10px 0px;
  font-size: 30rpx;
}

/* popup end */

.show {
  display: block;
}

.hidden {
  display: none;
}


.nothing-con {
  text-align: center;
  line-height: 250px;
}