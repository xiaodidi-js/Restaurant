/**index.wxss**/

/* .banner start */

.banner {
  position: relative;
}

.banner .swiper {
  width: 100%;
  height: 200px;
}

.banner .swiper .slide-image {
  width: 100%;
  height: 200px;
}

.icon-fangdajing {
  float: left;
  color: #ff8023;
  line-height: 40px;
  margin-left: 15px;
}

.banner .search {
  position: absolute;
  bottom: 30px;
  left: 0px;
  right: 0px;
  width: 80%;
  height: 40px;
  background: rgba(255, 255, 255, 0.9);
  border-radius: 20px;
  margin: 0px auto;
}

.banner .search .search-txt {
  height: 40px;
  line-height: 40px;
  width: 70%;
  color: #b5b5b6;
  margin-left: 40px;
  font-family: 'microsoft yahei';
}

/* .banner end */

.table {
  width: 100%;
}

.table .table-header {
  height: 45px;
  border-bottom: 1px solid #eee;
}

.table .table-header .tab {
  float: left;
  width: 30%;
  text-align: center;
  line-height: 43px;
  color: #595757;
  font-size: 30rpx;
}

.table .table-header .tab.active {
  width: 50%;
  height: 43px;
  color: #ff8023;
  border-bottom: 3px solid #ff8023;
}

/*左侧栏主盒子*/

.table .marTop {
  height: inherit;
}

.table .nav_left {
  display: block;
  width: 30%;
  /* height: calc(100% - 44px); */
  height: 100%;
  background: #f5f5f5;
  text-align: center;
  font-weight: bold;
  color: #9fa0a0;
  position: fixed;
  left: 0px;
  overflow-y: scroll;
}

/*左侧栏list的item*/

.table .nav_left .nav_left_items {
  width: 100%;
  height: 45.5px;
  line-height: 45.5px;
  border-top: 1px solid #dedede;
  font-size: 24rpx;
}

/*左侧栏list的item被选中时*/

.table .nav_left .nav_left_items.active {
  /*背景色变成白色*/
  color: #ff8023;
  background: #fff;
}

.table .nav_left .nav_left_items.active::before {
  content: '\e61c';
  padding-right: 5px;
}

/*右侧栏主盒子*/

.table .nav_right {
  position: absolute;
  /* top: 51px; */
  right: 0;
  flex: 1;
  width: 70%;
  height: auto;
  padding: 5px 0px 0px 5px;
  box-sizing: border-box;
  background: #fff;
}

.table .nav_right .nav_main {
  width: 100%;
  height: 75px;
  margin: 0px 0px 10px;
  padding: 5px 0px 10px;
  border-bottom: 1px solid #ccc;
}

